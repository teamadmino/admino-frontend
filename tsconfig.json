{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "admino-form": [
        "projects/admino-form/src/public-api"
      ],
      "admino-form/*": [
        "projects/admino-form/*"
      ],
      "admino": [
        "projects/admino/src/public-api"
      ],
      "admino/*": [
        "projects/admino/*"
      ],
    }
  }
}